{
  "name": "gitarist",
  "version": "1.0.7",
  "description": "A CLI tool to utilize Octokit",
  "license": "MIT",
  "packageManager": "pnpm@7.18.1",
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:thilllon/gitarist.git",
  "homepage": "https://github.com/thilllon/gitarist#readme",
  "bugs": {
    "url": "https://github.com/thilllon/gitarist/issues"
  },
  "contributors": [
    "thilllon <thilllon970@gmail.com>"
  ],
  "author": {
    "name": "thilllon",
    "email": "thilllon970@gmail.com",
    "url": "https://github.com/thilllon"
  },
  "funding": [
    {
      "type": "github",
      "url": "http://github.com/thilllon"
    },
    {
      "type": "ko_fi",
      "url": "https://ko-fi.com/thilllon"
    }
  ],
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "bin": {
    "gitarist": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx ./src/dev.ts",
    "build:tsc": "tsc",
    "build": "tsup",
    "start": "node ./dist/index.js",
    "format": "prettier --write --list-different .",
    "prerelease": "pnpm clean && pnpm format && pnpm lint && pnpm test:cov && pnpm build",
    "release": "release-it",
    "lint": "eslint --fix .",
    "test": "jest",
    "test:esm": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:cov": "jest --coverage",
    "clean": "rm -rf .artifacts",
    "prepare": "husky install && chmod +x .husky/*"
  },
  "dependencies": {
    "@octokit/types": "^9.0.0",
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "cosmiconfig": "^8.0.0",
    "dotenv": "^16.0.3",
    "fast-glob": "^3.2.12",
    "octokit": "^2.0.10",
    "octokit-plugin-create-pull-request": "^4.0.0",
    "ora": "^6.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@jest/globals": "^29.3.1",
    "@octokit/core": "^4.1.0",
    "@octokit/fixtures": "^22.0.6",
    "@octokit/plugin-rest-endpoint-methods": "^7.0.1",
    "@octokit/request-error": "^3.0.2",
    "@octokit/webhooks": "^10.3.1",
    "@swc/core": "^1.3.38",
    "@swc/jest": "^0.2.24",
    "@types/node": "^18.11.13",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "commitizen": "^4.2.6",
    "commitlint": "^17.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.4",
    "release-it": "^15.5.1",
    "supertest": "^6.3.3",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  }
}
