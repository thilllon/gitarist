{
  "search.exclude": {
    "coverage": true,
    "dist": true,
    "pnpm-lock.yaml": true,
    "**/node_modules": true,
    "**/*.code-search": true
  },
  "vitest.commandLine": "npx vitest",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  }
}
